{
	"name": "obsidian-github-stargazer",
	"version": "1.0.0",
	"description": "Sync and manage your GitHub starred repositories in Obsidian with notes, tags, and custom organization",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"github",
		"repositories",
		"stars"
	],
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=9.0.0"
	},
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/node": "^20.11.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"@vitest/coverage-v8": "^2.0.0",
		"@vitest/ui": "^2.0.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"tslib": "2.6.0",
		"typescript": "^5.3.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^2.0.0"
	},
	"dependencies": {
		"jiti": "^2.6.1",
		"obsidian": "latest"
	}
}
